<!DOCTYPE html>
<html lang="en">
<%- include("partials/head") %>

    <body>
        <%- include("partials/header") %>

            <main class="traffic-audit">
                <div class="page-title">
                    <h1>Traffic Audit Comparison</h1>
                </div>

                <% if (trafficData) { %>
                    <div class="companyA-title-grid">
                        <div class="companyA-title-flex">
                             <h2>
                            <%= trafficData.companyA.label %>
                        </h2>
                        </div>
                       
                    </div>

                    <div class="companyA-monthly-visits-charts">
                        <ul>
                            <% trafficData.companyA.monthlyVisits.forEach(element=> { %>
                                <li>
                                    <%= element.date %>:
                                        <%= element.visits %> visits
                                </li>
                                <% }); %>
                        </ul>
                    </div>
                    </div>

                    <div class="companyB-title">
                        <h2>
                            <%= trafficData.companyB.label %>
                        </h2>
                    </div>

                    <div class="companyB-monthly-visits-charts">
                        <ul>
                            <% trafficData.companyB.monthlyVisits.forEach(element=> { %>
                                <li>
                                    <%= element.date %>:
                                        <%= element.visits %> visits
                                </li>
                                <% }); %>
                        </ul>
                    </div>


                    </div>

                    </div>
                    <% } %>
            </main>
            <%- include('partials/footer') %>
    </body>

</html>